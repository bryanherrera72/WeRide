<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.weride.www.awsmobilehelper">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name=".auth.signin.SignInActivity"
            android:label="@string/title_activity_sign_in"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
            />
        <activity
            android:name=".auth.signin.userpools.ForgotPasswordActivity"
            android:label="@string/title_activity_forgot_password"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
            />
        <activity android:name=".auth.signin.userpools.MFAActivity"
            android:label="@string/title_activity_mfa"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"/>
        <activity android:name=".auth.signin.userpools.SignUpActivity"
            android:label="@string/title_activity_sign_up"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"/>
        <activity android:name=".auth.signin.userpools.SignUpConfirmActivity"
            android:label="@string/title_activity_sign_up_confirm"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"/>
    </application>

</manifest>