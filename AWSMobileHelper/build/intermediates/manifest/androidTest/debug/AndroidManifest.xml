<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.weride.www.awsmobilehelper.test" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="25" />

    <instrumentation
        android:name="android.support.test.runner.AndroidJUnitRunner"
        android:functionalTest="false"
        android:handleProfiling="false"
        android:label="Tests for com.weride.www.awsmobilehelper.test"
        android:targetPackage="com.weride.www.awsmobilehelper.test" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.0" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true" >
        <uses-library android:name="android.test.runner" />

        <activity
            android:name="com.weride.www.awsmobilehelper.auth.signin.SignInActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_sign_in" />
        <activity
            android:name="com.weride.www.awsmobilehelper.auth.signin.userpools.ForgotPasswordActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_forgot_password" />
        <activity
            android:name="com.weride.www.awsmobilehelper.auth.signin.userpools.MFAActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_mfa" />
        <activity
            android:name="com.weride.www.awsmobilehelper.auth.signin.userpools.SignUpActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_sign_up" />
        <activity
            android:name="com.weride.www.awsmobilehelper.auth.signin.userpools.SignUpConfirmActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_sign_up_confirm" />
    </application>

</manifest>